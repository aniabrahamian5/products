$.ajaxSetup({
    headers: {
        "X-CSRF-TOKEN": $('meta[name="csrf-token"]').attr("content"),
    },
});
if ($("#share").length != 0) {
    // $("#share").jsSocials({
    //     showLabel: false,
    //     showCount: false,
    //     shareIn: "popup",
    //     shares: ["facebook", "linkedin", "twitter"],
    // });
    var twitterShare = document.querySelector('[data-js="twitter-share"]');

    twitterShare.onclick = function (e) {
        e.preventDefault();
        var twitterWindow = window.open(
            "https://twitter.com/share?url=" + window.location,
            "twitter-popup",
            "height=350,width=600"
        );
        if (twitterWindow.focus) {
            twitterWindow.focus();
        }
        return false;
    };

    var facebookShare = document.querySelector('[data-js="facebook-share"]');

    facebookShare.onclick = function (e) {
        e.preventDefault();
        var facebookWindow = window.open(
            "https://www.facebook.com/sharer/sharer.php?u=" + window.location,
            "facebook-popup",
            "height=350,width=600"
        );
        if (facebookWindow.focus) {
            facebookWindow.focus();
        }
        return false;
    };

    var linkedinShare = document.querySelector('[data-js="linkedin-share"]');

    linkedinShare.onclick = function (e) {
        e.preventDefault();
        var linkedinWindow = window.open(
            "https://www.linkedin.com/sharing/share-offsite/?url=" +
                window.location,
            "linkedin-popup",
            "height=350,width=600"
        );
        if (linkedinWindow.focus) {
            linkedinWindow.focus();
        }
        return false;
    };
}

$(window).scroll(function () {
    var scroll = $(window).scrollTop();
    if (scroll >= 700 && scroll < $(document).height()) {
        $("#share").addClass("share-div");
    } else {
        $("#share").removeClass("share-div");
    }
});
$(".items-blog").slice(0, 10).show();
$(document).scroll(function () {
    $(".items-blog:hidden").slice(0, 9).slideDown();
});
$(".filter-icon").click(function () {
    $("#search-mobile").removeClass("hide");
});
$(".close-modal-btn").click(function () {
    $("#search-mobile").addClass("hide");
});
function clearcart() {
    var prefix_lang = $("#prefix_lang").val();
    $.post("/cart/clear/", "json", function (data) {
        window.location.href = "/" + prefix_lang + "products";
    });
}
function addTocartnew(product_id) {

    var qty = $("#quantity-cart-" + product_id).val();
    if (qty != 0) {
        $.post("/cart/edit/" + product_id + "/" + qty, "json", function (data) {
            if (data != 0) {
                var html = "";
                var datajson = JSON.parse(data);
                var i = 0;
                datajson.forEach(function (entry) {
                    i += parseInt(entry.quantity);
                });
                $(".cart-badge").html(i),
                location.reload();
            }
        });
    } else {
        removeCart(product_id);
        adddeleteproduct();
    }
}
function adddeleteproduct() {
    var product_id = $("#delete-cart-input").val();
    var qty = $("#quantity-cart-" + product_id).val();
    $.post("/cart/add/" + product_id + "/0", "json", function (data) {
        if (data != 0) {
            var html = "";
            var datajson = JSON.parse(data);
            var i = 0;
            datajson.forEach(function (entry) {
                i += parseInt(entry.quantity);
            });
            $(".div-li-3 a").html("View cart(" + i + ")");
            $(".view-cart-mobile a").html("View cart(" + i + ")");
            if (i != 0) {
                location.reload();
            } else {
                window.location.href = "/products";
            }
        }
    });
}
function removeCart(product_id) {
    $("#delete-cart-input").val(product_id);
}
function addTocartOwl(product_id) {
    $.post("/cart/add/" + product_id + "/1", "json", function (data) {
        if (data != 0) {
            var html = "";
            var datajson = JSON.parse(data);
            var i = 0;
            datajson.forEach(function (entry) {
                i += parseInt(entry.quantity);
            });
            $(".div-li-3 a").html("View cart(" + i + ")");
            $(".view-cart-mobile a").html("View cart(" + i + ")");
            location.reload();
        }
    });
}
function gotopayment() {
    var shipping_name = $(".shipping-name").html();
    if ($(".shipping-company").html() != "") {
        var shipping_company = $(".shipping-company").html();
    } else {
        var shipping_company = "0";
    }
    var shipping_phone = $(".shipping-phone").html();
    var shipping_address = $(".shipping-address").html();
    if ($(".shipping-bld").html() != "") {
        var shipping_bld = $(".shipping-bld").html();
    } else {
        var shipping_bld = "0";
    }
    var shipping_city = $(".shipping-city").html();
    var shipping_country = $(".shipping-country").html();
    // billing part
    if ($(".billing-name").html() != "") {
        var billing_name = $(".billing-name").html();
    } else {
        var billing_name = "0";
    }
    if ($(".billing-company").html() != "") {
        var billing_company = $(".billing-company").html();
    } else {
        var billing_company = "0";
    }
    if ($(".billing-phone").html() != "") {
        var billing_phone = $(".billing-phone").html();
    } else {
        var billing_phone = "0";
    }
    if ($(".billing-address").html() != "") {
        var billing_address = $(".billing-address").html();
    } else {
        var billing_address = "0";
    }
    if ($(".billing-bld").html() != "") {
        var billing_bld = $(".billing-bld").html();
    } else {
        var billing_bld = "0";
    }
    if ($(".billing-city").html() != "") {
        var billing_city = $(".billing-city").html();
    } else {
        var billing_city = "0";
    }
    if ($(".billing-country").html() != "") {
        var billing_country = $(".billing-country").html();
    } else {
        var billing_country = "0";
    }
    if (
        shipping_name != "" &&
        shipping_phone != "" &&
        shipping_address != "" &&
        shipping_city != "" &&
        shipping_country != ""
    ) {
        $(".error-message").hide();
        $.post(
            "/gopayment/" +
                shipping_name +
                "/" +
                shipping_phone +
                "/" +
                shipping_address +
                "/" +
                shipping_city +
                "/" +
                shipping_country +
                "/" +
                shipping_bld +
                "/" +
                shipping_company +
                "/" +
                billing_name +
                "/" +
                billing_phone +
                "/" +
                billing_address +
                "/" +
                billing_city +
                "/" +
                billing_country +
                "/" +
                billing_bld +
                "/" +
                billing_company,
            "json",
            function (data) {
                if (data != 0) {
                    var prefix_lang = $("#prefix_lang").val();
                    window.location.assign(
                        "/" + prefix_lang + "checkout-review"
                    );
                }
            }
        );
    } else {
        $(".error-message").show();
    }
}
function editbilling() {
    if (
        $("#billing-name").val() != "" &&
        $("#billing-username").val() != "" &&
        $("#billing-phone").val() != "" &&
        $("#billing-street").val() != "" &&
        $("#billing-zip").val() != "" &&
        $("#statesbilling").val() != "" &&
        $("#citiesbilling").val() != ""
    ) {
        var billing_country = "";
        var billing_state = "";
        var billing_city = "";
        $(".billing-name").html(
            $("#billing-name").val() + " " + $("#billing-username").val()
        );
        $(".billing-company").html($("#billing-company").val());
        $(".billing-address").html($("#billing-street").val());
        $(".billing-bld").html($("#billing-bld").val());
        $("#statesbilling option").each(function () {
            if ($(this).attr("value") == $("#statesbilling").val()) {
                billing_state = $(this).html();
            }
        });
        billing_city = $("#citiesbilling").val();
        billing_country = "US";
        $(".billing-city").html(
            billing_city + ", " + billing_state + " " + $("#billing-zip").val()
        );
        $(".billing-country").html(billing_country);
        $(".billing-phone").html($("#billing-phone").val());
        $(".error-message").hide();
        gotopayment();
    } else {
        $(".error-message").show();
    }
}
function editbillingShipping() {
    if (
        $("#shipping-name").val() != "" &&
        $("#shipping-username").val() != "" &&
        $("#shipping-phone").val() != "" &&
        $("#shipping-street").val() != "" &&
        $("#shipping-zip").val() != "" &&
        $("#shipping-country").val() != "" &&
        $("#states").val() != "" &&
        $("#cities").val() != ""
    ) {
        var shipping_country = "";
        var shipping_state = "";
        var shipping_city = "";
        $(".shipping-name").html(
            $("#shipping-name").val() + " " + $("#shipping-username").val()
        );
        $(".shipping-company").html($("#shipping-company").val());
        $(".shipping-address").html($("#shipping-street").val());
        $(".shipping-bld").html($("#shipping-bld").val());
        $("#states option").each(function () {
            if ($(this).attr("value") == $("#states").val()) {
                shipping_state = $(this).html();
            }
        });
        shipping_city = $("#cities").val();
        shipping_country = "US";
        $(".shipping-city").html(
            shipping_city +
                ", " +
                shipping_state +
                " " +
                $("#shipping-zip").val()
        );
        $(".shipping-country").html(shipping_country);
        $(".shipping-phone").html($("#shipping-phone").val());
        $("#shipping-address").fadeIn();
        $("#billing-address").fadeIn();
        $("#btn-checkout").fadeIn();
        $("#shipping-form").fadeOut();
        $(".error-message").hide();
        gotopayment();
    } else {
        $(".error-message").show();
    }
}

function sendReview() {
    if ($("input:radio[name=rating]:checked").val()) {
        var rating = $("input:radio[name=rating]:checked").val();
    } else {
        var rating = 1;
    }
    if ($("#review-title").val() == "") {
        $("#review-title").addClass("error-cart");
    } else {
        $("#review-title").removeClass("error-cart");
    }
    if ($("#review-comment").val() == "") {
        $("#review-comment").addClass("error-cart");
    } else {
        $("#review-comment").removeClass("error-cart");
    }
    if ($("#review-name").val() == "") {
        $("#review-name").addClass("error-cart");
    } else {
        $("#review-name").removeClass("error-cart");
    }
    if ($("#review-email").val() == "") {
        $("#review-email").addClass("error-cart");
    } else {
        $("#review-email").removeClass("error-cart");
    }

    if (
        $("#review-title").val() != "" &&
        $("#review-comment").val() != "" &&
        $("#review-name").val() != "" &&
        $("#review-email").val() != ""
    ) {
        $(".feedback-part-1").fadeOut();
        $(".feedback-part-2").fadeIn();
        $(".thanks-review").html(
            "Dear " +
                $("#review-name").val() +
                ",<br>Your Review Has been Received. Thank you."
        );
        $.post(
            "/review/" +
                $("#review-title").val() +
                "/" +
                rating +
                "/" +
                $("#review-comment").val() +
                "/" +
                $("#review-name").val() +
                "/" +
                $("#review-email").val() +
                "/" +
                $("#review-product").val(),
            "json",
            function (data) {}
        );
    }
}
function sendFeedback() {
    if ($("#feedback-email").val() == "") {
        $("#feedback-email").addClass("error-cart");
    } else {
        $("#feedback-email").removeClass("error-cart");
    }
    if ($("#feedback-name").val() == "") {
        $("#feedback-name").addClass("error-cart");
    } else {
        $("#feedback-name").removeClass("error-cart");
    }
    if ($("#feedback-description").val() == "") {
        $("#feedback-description").addClass("error-cart");
    } else {
        $("#feedback-description").removeClass("error-cart");
    }
    if (
        $("#feedback-email").val() != "" &&
        $("#feedback-description").val() != ""
    ) {
        $(".feedback-part-1").fadeOut();
        $(".feedback-part-2").fadeIn();
        $(".thanks-feedback").html(
            "Dear " +
                $("#feedback-name").val() +
                ",<br>Thank you for your feedback. We will review your feedback and act accordingly."
        );
        $.post(
            "/feedback/" +
                $("#feedback-product").val() +
                "/" +
                $("#feedback-email").val() +
                "/" +
                $("#feedback-name").val() +
                "/" +
                $("#feedback-description").val(),
            "json",
            function (data) {}
        );
    }
}

function downloadCad(el) {
    var link = document.createElement("a");
    link.download = name;
    link.href = $(el).parent().parent().find(".selectedCad").val();
    link.click();
}

