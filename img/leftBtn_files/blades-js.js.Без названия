    });
$(".btn-quote").click(function () {
    $("#loading-call").show();
    $(this).closest("form").submit();
});
function setshippings(x) {
    var price = $(x).attr("data-payment");
    var placetype = $(x).attr("data-ups");
    $("#shipping_price").html("$" + price);
    var all = $("#prod-total").html().split("$")[1];
    var all_price =
        parseFloat(all) +
        parseFloat(price) +
        parseFloat($("#tax_price").attr("data-val")) +
        parseFloat($("#expedite_price").attr("data-val"));
    $("#all_price").html("$" + all_price.toFixed(2));
    $("#place_type").val(placetype);
}

function setColumnSort(sort, sort_by, sort_icon) {
    $(".sort-item").val(sort);
    $(".sort-by-item").val(sort_by);
    $(".search-page").submit();
}
$(document).ready(function () {
    if (window.location.href.indexOf("sub-category") > -1) {
        var windowHeight = $(document).height();
        let check = 1;
        $(window).scroll(function () {
            console.log(
                $(this).scrollTop() > windowHeight - 4000,
                $(".allcount_data").val()
            );

            if (
                $(this).scrollTop() > windowHeight - 4000 &&
                check == 1 &&
                $(".allcount_data").val() != "0"
            ) {
                check = 0;
                let sort = "ASC";
                let sort_by = "id";
                let skip = Number($(".skip").val());
                if ($(".sort-by-item").val()) {
                    sort = $(".sort-by-item").val();
                }
                if ($(".sort-item").val()) {
                    sort_by = $(".sort-item").val();
                }
                let query_new = window.location.href;
                query_new = query_new.split("q=&")[1]
                    ? query_new.split("q=&")[1]
                    : 0;
                let parent_cat = $(".parent_cat_id").val()
                    ? $(".parent_cat_id").val()
                    : 0;
                let is_cad =  window.location.href.split("?is_cad=")[1]
                    ? window.location.href.split("?is_cad=")[1]
                    : 0;                    
                $.ajax({
                    type: "GET",
                    contentType: "application/json",
                    url:
                        "/loaddata/" +
                        $(".subcat_id").val() +
                        "/" +
                        parent_cat +
                        "/" +
                        $(".last_product_id").val() +
                        "/" +
                        sort +
                        "/" +
                        sort_by +
                        "/" +
                        skip +
                        "/" +
                        query_new +
                        "/" + 
                        is_cad,
                    success: function (data) {
                        $(".skip").val(Number($(".skip").val()) + 20);
                        check = 1;
                        console.log(data);

                        if (data.products) {
                            $(".allcount_data").val(data.products.length);
                        } else {
                            $(".allcount_data").val(0);
                        }
                        item = data.products;
                        col = data.products_columns;
                        pr_price = data.pr_price;

                        $.each(item, function (index, v) {
                            let options = "";
                            if (
                                item[index].cad_names != null &&
                                item[index].cad_names.length != 0
                            ) {
                                $.each(
                                    item[index].cad_names.split(","),
                                    function (i2, v2) {
                                        options +=
                                            ' <option value="/cad_files/' +
                                            item[index].cad_files.split(",")[
                                                i2
                                            ] +
                                            '">' +
                                            v2 +
                                            "</option>";
                                    }
                                );
                            } else if (
                                item[index].cad_files != null &&
                                item[index].cad_files.length != 0
                            ) {
                                $.each(
                                    item[index].cad_files.split(","),
                                    function (i2, v2) {
                                        options +=
                                            ' <option value="/cad_files/' +
                                            item[index].cad_files.split(",")[
                                                i2
                                            ] +
                                            '">' +
                                            v2 +
                                            "</option>";
                                    }
                                );
                            }
                            let html =
                                '<tr class="new-data with-cads item-tr-' +
                                item[index].id +
                                '" style="cursor:pointer">';
                            $.each(col, function (i, v1) {
                                i == col.length - 1
                                    ? (html +=
                                          '<td class="cyan" style="cursor:pointer" onclick="showProduct(' +
                                          item[index].id +
                                          "," +
                                          col[i].field_id +
                                          ',$(this))">' +
                                          pr_price[
                                              "title" +
                                                  item[index].id +
                                                  "-" +
                                                  col[i].field_id
                                          ] +
                                          "</td>")
                                    : (html +=
                                          '<td onclick="showProduct(' +
                                          item[index].id +
                                          "," +
                                          col[i].field_id +
                                          ',$(this))">' +
                                          pr_price[
                                              "title" +
                                                  item[index].id +
                                                  "-" +
                                                  col[i].field_id
                                          ] +
                                          "</td>");
                            });
                            let html1 =
                                '<td class="hide product-item-' +
                                item[index].id +
                                ' product-tds">' +
                                '<div class="product-info col-sm-12">' +
                                '<div class="row">' +
                                '<div class="col-sm-3 shadow-cabinet">' +
                                '<a href="/product/' +
                                item[index].id +
                                '">' +
                                '<img alt="' +
                                item[index].alt +
                                '" src="/images/frontend_images/products/' +
                                item[index].filename.split(",")[0] +
                                '" class="product_image image' +
                                item[index].id +
                                '">' +
                                "</a>" +
                                "</div>" +
                                '<div class="col-sm-4">' +
                                '<div class="showing-p">' +
                                '<a href="/product/' +
                                item[index].id +
                                '" style="color: #000;text-decoration: none;">' +
                                ' <p><b style="color:#858282">' +
                                item[index].title +
                                "</b></p>" +
                                " <p>MPN: " +
                                item[index].mpn +
                                "</p>" +
                                ' <p class="gray-p">' +
                                (item[index].description.length > 150
                                    ? item[index].description.substr(0, 150) +
                                      "..."
                                    : item[index].description) +
                                '<span class="gray-p">' +
                                '<span class="cyan">' +
                                $(".mess_readmore").val() +
                                "</span>" +
                                "</span>" +
                                "</p>" +
                                "</a>" +
                                "</div>" +
                                " </div>" +
                                '<div class="col-sm-2">' +
                                (((item[index].cost == 0 ||
                                    item[index].price == 0) &&
                                    item[index].in_stock != 1) ||
                                item[index].parent_brand == 67 ||
                                item[index].parent_brand == 113 ||
                                item[index].cat_id == 312 ||
                                item[index].cat_id == 313 ||
                                item[index].cat_id == 315 ||
                                item[index].cat_id == 124 ||
                                item[index].cat_id == 228 ||
                                item[index].parent_brand == 14 ||
                                item[index].brand_id == 1
                                    ? ""
                                    : item[index].in_stock != 1
                                    ? "<br><br>"
                                    : '<p class="price-p">$' +
                                      Number(item[index].price).toFixed(2) +
                                      "</p>") +
                                '<input type="number" name="count" value="1" class="form-control number-count" min="1" id="number-count-' +
                                item[index].id +
                                '">' +
                                ' <p class="availability">' +
                                $(".mess_availability").val() +
                                ":</p>" +
                                "</div>" +
                                '<div class="col-sm-3">' +
                                '<p class="padding-45"></p>' +
                                (item[index].cost == 0 ||
                                item[index].price == 0 ||
                                item[index].parent_brand == 67 ||
                                item[index].parent_brand == 113 ||
                                item[index].cat_id == 312 ||
                                item[index].cat_id == 313 ||
                                item[index].cat_id == 315 ||
                                item[index].cat_id == 124 ||
                                item[index].cat_id == 228 ||
                                item[index].parent_brand == 14 ||
                                item[index].brand_id == 1
                                    ? '<a href="/' +
                                      $(".prefix").val() +
                                      "search?product=" +
                                      item[index].id +
                                      '" class="add-cart request-quote">' +
                                      $(".mess_reqaquote").val() +
                                      "</a>"
                                    : '<button type="button" name="button" class="add-cart" onclick="addTocart(' +
                                      item[index].id +
                                      ');">' +
                                      $(".mess_addtocart").val() +
                                      "</button>") +
                                '<div class="gray-div">' +
                                (!(
                                    item[index].parent_brand == 67 ||
                                    item[index].parent_brand == 113 ||
                                    item[index].cat_id == 312 ||
                                    item[index].cat_id == 313 ||
                                    item[index].cat_id == 315 ||
                                    item[index].cat_id == 124 ||
                                    item[index].cat_id == 228 ||
                                    item[index].parent_brand == 14 ||
                                    item[index].brand_id == 1
                                )
                                    ? item[index].in_stock == 1
                                        ? "<p>" +
                                          $(".mess_stock").val() +
                                          '</p><span class="cyan">' +
                                          item[index].message_call +
                                          "</span>"
                                        : item[index].cost == 0 ||
                                          item[index].price == 0 ||
                                          item[index].parent_brand == 67 ||
                                          item[index].parent_brand == 113 ||
                                          item[index].cat_id == 312 ||
                                          item[index].cat_id == 313 ||
                                          item[index].cat_id == 315 ||
                                          item[index].cat_id == 124 ||
                                          item[index].cat_id == 228 ||
                                          item[index].parent_brand == 14 ||
                                          item[index].brand_id == 1
                                        ? ""
                                        : item[index].message_call.length == 0
                                        ? '<a href="#" class="underline" data-toggle="modal" data-target="#please-call">' +
                                          $(".mess_call").val() +
                                          "</a>"
                                        : '<span class="cyan">' +
                                          item[index].message_call +
                                          "</span>"
                                    : "") +
                                (item[index].alternativs &&
                                item[index].alternativs.length != 0
                                    ? '<a href="/product/' +
                                      item[index].id +
                                      '#alternative" class="underline">' +
                                      $(".mess_alternative").val() +
                                      "</a>"
                                    : "") +
                                "</div>" +
                                "</div>" +
                                "</div>" +
                                (item[index].cad_files != null &&
                                item[index].cad_files.length != 0
                                    ? '<div class="cad-block">' +
                                      '<div class="row">' +
                                      '<div class="col-sm-5"></div>' +
                                      '<div class="col-sm-4">' +
                                      '<select class="form-control selectedCad"  class="" style="height: 52px;">' +
                                      options +
                                      " </select>" +
                                      "</div>" +
                                      '<div class="col-sm-3">' +
                                      '<button type="button" name="button" class="btn btn-checkout" onclick="downloadCad($(this))">' +
                                      $(".mess_download").val() +
                                      "</button>" +
                                      "</div>" +
                                      "</div>" +
                                      "</div>"
                                    : "") +
                                " </div>" +
                                " </td>" +
                                "</tr>";
                            html3 = html + html1;

                            $(".table").find("tbody").append(html3);

                            $(".last_product_id").val(item[item.length - 1].id);
                            $(".product-tds").attr("style", "top:80px");
                        });
                    },
                    error: function (data, status, xhr) {
                        console.log(data);
                    },
                });
            }
        });
    }
